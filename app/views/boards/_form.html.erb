<%= simple_form_for(@board) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :admin_user_id %>
    <%= f.input :read_perm %>
    <%= f.input :write_perm %>
    <%= f.input :group_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
