<%= simple_form_for(@post) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :board_id %>
    <%= f.input :user_id %>
    <%= f.input :b_category_id %>
    <%= f.input :title %>
    <%= f.input :contents %>
    <%= f.input :addlink %>
    <%= f.input :isnotice %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
